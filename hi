#include <iostream>
#include <vector>
#include <cmath>
using namespace std;

int main() {
    int H, W, N;
    cin >> H >> W >> N;
    
    vector<int> canvas(W, 0); 
    for (int i = 0; i < N; i++) {
        int r, c, t, x;
        cin >> r >> c >> t >> x;
        for (int pos = 0; pos < W; pos++) {
            if (abs(pos - c) <= t) {
                canvas[pos] += x;
            }
        }
    }
    
    for (int i = 0; i < W; i++) {
        cout << canvas[i];
        if (i != W - 1) cout << " ";
    }
    cout << "\n";
    
    return 0;
}